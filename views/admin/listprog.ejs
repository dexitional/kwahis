													<a href="/program/del/<%- row.id %>" title="Delete Program" style="margin-right:5px;" class="btn btn-danger btn-sm"  onclick="return confirm('Delete Program?')"><i class="fa fa-trash"></i></a>
												</div>
											    <% }%>
											</td>                                                                                         
									   </tr>
									<% }) %>
								  </tbody>
							   </table>
							  
						  </div>
