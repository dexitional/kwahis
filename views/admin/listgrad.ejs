		<script>
			// Messages on Records 
		    <% if(msg === 'saved'){ %>
				  toastr.success('', 'Record Saved Successfully!');					 
			<% }%>

             <% if(msg === 'deleted'){ %>
				  toastr.success('', 'Record Deleted Successfully!');					 
			<% }%>

			<% if(msg === 'failed'){ %>
				toastr.danger('', 'Saving failed!');					 
			<% }%>		
			
			<% if(msg === 'noscript'){ %>
				toastr.success('', 'No Transcript Records for Student');
				window.close();									 
			<% }%>	
			<% if(msg === 'nots'){ %>
				toastr.success('', 'No Payment Transactions recorded on Student');
				window.close();									 
			<% }%>	

			
		</script>


