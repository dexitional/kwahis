				    	 <div class="table-responsive">
							  
							  <table class="table table-bordered table-striped table-hover dataTable js-exportable">
								  <thead>
									  <tr role="row">
										  <th>ID</th>
										  <th width="200px">Program</th>
										  <th>Year Started</th>
										  <th width="600px">Structure</th>
										  <th width="300px">Levels</th>	
										  <th width="300px">Prefix</th>	
										  <th width="300px">Scoring</th>
										  <th width="300px">Order</th>										  
										  <th width="100px">Action</th>
									  </tr>
								  </thead>								  
								  <tbody> 
									
									<% rows.map((row,i) => { %>
									  <tr class="odd">											
											<td><b><%- row.id %></b></td>
											<td><b><%- row.name %><b></td>
											<td><b><%- row.year_started %></b></td>	
											<td><b><%- row.print %></b></td>
											<td><b><%- row.levels %></b></td>
											<td><b><%- row.prefix %></b></td>
											<td><b><%- row.scheme %></b></td>
											<td><b><%- row.order %></b></td>
											<td>
